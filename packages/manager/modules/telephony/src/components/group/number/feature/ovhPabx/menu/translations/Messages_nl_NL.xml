<!-- READ DTMF -->
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_read_dtmf" qtlid="464110">Variable d'entrée DTMF</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_read_dtmf_explain" qtlid="464123">Permet, par exemple, de récupérer un identifiant client.</translation>
