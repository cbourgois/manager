<!-- SET CALLER NAME -->
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_set_caller_name" qtlid="324535">Définir un nom d'appelant</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_set_caller_name_explain" qtlid="463928">Permet de changer le nom de l'appelant qui sera présenté sur le téléphone de la ligne qui décroche l'appel.</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_set_caller_name_action_param" qtlid="463941">Nom de l'appelant</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_set_caller_name_action_param_required" qtlid="463954">Veuillez donner un nom d'appelant.</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_set_caller_name_action_param_bad_format" qtlid="463967">Le nom de l'appelant ne peut contenir que des lettres (sans accents), chiffres, tirets bas ou des espaces.</translation>
<!-- BRIDGE -->
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_bridge" qtlid="432163">Transférer l'appel</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_bridge_explain" qtlid="463980">Permet simplement de transférer l'appel vers une ligne.</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_bridge_to_number" qtlid="463993">Transférer vers le numéro</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_bridge_action_param_required" qtlid="464006">Veuillez spécifier un numéro vers lequel transférer l'appel.</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_bridge_action_param_bad_format" qtlid="464019">Le numéro vers lequel transférer l'appel doit être au format international (ex: +33312345678).</translation>
<!-- CALLCENTER -->
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_callcenter" qtlid="464032">Faire sonner les membres de la file</translation>
   <translation id="telephony_number_feature_ovh_pabx_menu_entry_action_callcenter_explain" qtlid="464045">Permet de sélectionner des membres et de les faire sonner simultanément.</translation>
